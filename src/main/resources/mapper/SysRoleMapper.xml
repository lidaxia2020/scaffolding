<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scaffolding.demo.mapper.SysRoleMapper">

    <insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="com.scaffolding.demo.entity.SysRole">
        insert into sys_role (role_name, remark) value (#{roleName}, #{remark})
    </insert>
</mapper>